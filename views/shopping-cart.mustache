<form class="shop-form" method="post" action="/shop-form" name="shop-from" id="shop-form">
	<section class="content-shopping-cart-page">
		<div class="container">
				<div class="wrap-table-shopping-cart">
					<table class="table-shopping-cart">
						<tr class="table_head">
							<th class="column-1">Product</th>
							<th class="column-2">Name & Description</th>
							<th class="column-3">Price</th>
							<th class="column-4">Quantity</th>
							<th class="column-5">Total</th>
							<th class="column-6"></th>
						</tr>
                     
                         {{#user.items}}
 						<tr>
      							<td class="column-1">
 								<div class="img-product">
									<img src="images/shopping-cart-img-product-in-table-01.jpg" alt="img-product">
								</div>
  							</td>
							<td class="column-2" name="one" type="text" id="item" value={{0}}>{{0}}</td>
							<td class="column-3" name="two" type="text" id="price">{{1}}</td>
							<td class="column-4">
								<input class="input-num-product small-text" type="text" name="num-product" id="quantity" value = {{2}}>
							</td>
 							<td class="column-5" type="text" id="something">{{3}} </td> 
 							<td class="column-6">
								<i class="btn-remove-product fa fa-times" aria-hidden="true"></i>
 							</td>
   						</tr>
                        {{/user.items}}      
 					</table>
				</div>
                
                
              <section>  
                <form class="coupon-form" method="post" action="/coupon-form" name="coupon-from" id="coupon-form">
				<div class="wrap-coupon">
					<div class="row">
						<div class="wrap-title-coupon col-responsive-shopping-cart col-md-10 col-lg-10">
							<h4 class="title-coupon">Coupon Code</h4>
						</div>

						<div class="wrap-input-coupon col-responsive-shopping-cart col-md-10 col-lg-10">
							<input class="input-coupon small-text" name="your-coupon" type="text" id="your-coupon" placeholder="Enter your coupon here">
						</div>
						<div class="wrap-btn-coupon col-responsive-shopping-cart col-md-10 col-lg-2">
							<button class="btn-coupon btn-with-bg" type="submit" >Apply Coupon</button>
						</div>
					</div>
				</div>
                </form>
                </section>
                
				<div class="wrap-estimate-shipping-extend wrap-coupon">
					<div class="row">
						<div class="wrap-title-coupon col-responsive-shopping-cart col-md-10 col-lg-10">
							<h4 class="title-coupon" name="shipping" type="text" id="shipping">Estimate Shipping</h4>
						</div>

						<div class="wrap-chose-address-extend wrap-input-coupon col-responsive-shopping-cart col-md-10 col-lg-10">
							<div class="row">
								<div class="col-responsive-chose-address left col-md-12 col-lg-6">
									<select class="chose-people" name="chose-city">
										<option>Ball Park 31st Street</option>
										<option>Norway Plane 15th main</option>
										<option>Hollywood 7th main</option>
										<option>West Street 31st main</option>
										<option>Woodlane 1st street</option>
										<option>Mirage West 12th main</option>
									</select>
								</div>

								<div class="col-responsive-chose-address right  col-md-12 col-lg-6">
									<select class="chose-time" name="chose-state">
										<option>New York</option>
										<option>New Jersey</option>
										<option>California</option>
										<option>Florida</option>
										<option>Iowa</option>
										<option>Texas</option>
									</select>
								</div>
							</div>
						</div>
						
						<div class="wrap-input-coupon col-responsive-shopping-cart col-md-10 col-lg-10">
							<input  type="text" class="input-coupon small-text" name="your-pin" type="text" id="your-pin" placeholder="Enter your post code here">
						</div>
					</div>
				</div>
    
				<div class="wrap-total clearfix">
						<div class="style-pos col-right">
							<h6 class="larg-text" name="total" type="text" value=>Total: $  {{user.total}}</h6>
							<button class="btn-with-bg">Estimate Delivery</button><p></p>
                            <button class="btn-with-bg"><a href="check-out.mustache">Proceed to checkout</button></a>
						</div>
						<div class="style-pos col-left">
							<a href="shop-page.mustache" class="small-text"><i class="fa fa-angle-left" aria-hidden="true"></i> Continue Shopping</a>
						</div>
				</div>
            </form>
		</div>
	</section>

